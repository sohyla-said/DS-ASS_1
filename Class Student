#include <iostream>
using namespace std;

class Student{
private:
    string id;
    string name;
    double gpa;
public:
    //constructor
    Student(string studentId, string studentName, double studentGpa) {
        id = studentId;
        name = studentName;
        gpa = studentGpa;
    }
    //Overloading operator <
    bool operator<(const Student& other) const {
        return name < other.name;
    }
    //friend function for printing
    friend ostream& operator<<(ostream& os, const Student& s) {
        os << "ID: " << s.id << ", Name: " << s.name << ", GPA: " << s.gpa;
        return os;
    }
    string getID()const{
        return id;
    }
    string getNAME()const{
        return name;
    }
    const double getGPA()const{
        return gpa;
    }

};
